<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CoreNetworkStateLayout">
        <attr name="core_loading" format="reference"/>
        <attr name="core_error" format="reference"/>
        <attr name="core_empty" format="reference"/>
    </declare-styleable>
    <declare-styleable name="CorePlaceholderImageView">
        <attr name="core_placeholder" format="reference"/>
    </declare-styleable>
    <declare-styleable name="CoreToolbar">
        <attr name="core_hasStatusBar" format="boolean"/>
        <attr name="core_hasBack" format="boolean"/>
        <attr name="core_hasBottomLine" format="boolean"/>
<!--        <attr name="isWhiteStyle" format="boolean"/>-->
        <attr name="core_hasElevation" format="boolean"/>
        <attr name="core_tbBackground" format="color"/>
<!--        <attr name="hasSettings" format="boolean"/>-->
<!--        <attr name="hasConfirm" format="boolean"/>-->
<!--        <attr name="hasWebClose" format="boolean"/>-->
    </declare-styleable>
    <declare-styleable name="CornerView">
        <attr name="cv_color" format="color"/>
        <attr name="cv_direction" format="enum">
            <enum name="leftTop" value="0"/>
            <enum name="leftBottom" value="1"/>
            <enum name="rightTop" value="2"/>
            <enum name="rightBottom" value="3"/>
        </attr>
    </declare-styleable>
</resources>